<script type="text/javascript" src="../../../public/js/bioFunctions.js"></script>
<div class="form-bio-link" style="display: none;">
    <div class="row g-0">
        <div class="col-md-2 bg-secondary">
            tes
        </div>
        <div class="col-md-10 bg-primary">
            <div class="ps-3">
                <h4>Title & Description</h4>
                <div class="mb-3">
                    <input required type="text" name="title" id="titleUrl"  placeholder="title" class="form-control">
                </div>
                <div class="mb-3">
                    <input required type="text" name="Description" id="description"  placeholder="Description" class="form-control">
                </div>
            </div>
        </div>
    </div>
    <h4>Destination</h4>
    <div class="mb-3">
        <input required type="url" name="destination" id="fullUrl"  placeholder="destination" class="form-control" value="<%= data.full %>">
    </div>
    
    <div class="d-flex flex-row-reverse">
        <button type="button" onclick="btnAddUrl(this)" class="btn btn-primary ms-3">Submit</button>
        <button type="button" onclick="btnCancel(this)" class="btn btn-danger" value="url">Cancel</button>
    </div>
</div>