
<script type="text/javascript" src="../../../public/js/loginRegisterFunctions.js"></script>
<main class="form-signin">
    <form>
        <img class="mb-4" src="https://storage.googleapis.com/url-shortener-storage-2/logo-shortlink.webp" alt="" width="72" height="72">
        <h1 class="h3 mb-3 fw-normal">Please sign in</h1>
    
        <div class="form-floating">
        <input type="email" class="form-control" id="email" placeholder="name@example.com">
        <label for="floatingInput">Email address</label>
        </div>
        <div class="form-floating">
        <input type="password" class="form-control" id="password" placeholder="Password">
        <label for="floatingPassword">Password</label>
        </div>
    
        <button onclick="login(this)" class="w-100 btn btn-lg btn-primary" type="button">Sign in</button>
        <p class="mt-2">Don't have an Account?</p>
        <a href="/register"><button class="w-100 btn btn-lg btn-secondary" type="button">Register</button></a>
        <p class="mt-5 mb-3 text-muted">&copy; 2024</p>
    </form>
</main>

  